{
  "name": "@trpc-playground/query-extension",
  "version": "0.1.0",
  "type": "module",
  "description": "parse query syntax for trpc-playground",
  "license": "Apache-2.0",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
