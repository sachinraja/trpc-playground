{
  "name": "@trpc-playground/components",
  "type": "module",
  "version": "0.0.0",
  "description": "core components for trpc-playground",
  "repository": {
    "directory": "packages/components",
    "type": "git",
    "url": "https://github.com/sachinraja/trpc-playground"
  },
  "homepage": "https://github.com/sachinraja/trpc-playground#readme",
  "bugs": {
    "url": "https://github.com/sachinraja/trpc-playground/issues"
  },
  "author": "Sachin Raja <sachinraja2349@gmail.com>",
  "license": "MIT",
  "keywords": [
    "trpc",
    "playground"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "tsup",
    "build:types": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm build",
    "watch": "tsup --watch --no-dts",
    "type-check": "tsc"
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.19.9",
    "@codemirror/closebrackets": "0.19.0",
    "@codemirror/commands": "0.19.6",
    "@codemirror/comment": "0.19.0",
    "@codemirror/gutter": "0.19.9",
    "@codemirror/history": "0.19.1",
    "@codemirror/lang-javascript": "0.19.3",
    "@codemirror/lang-json": "0.19.1",
    "@codemirror/language": "0.19.7",
    "@codemirror/lint": "0.19.3",
    "@codemirror/matchbrackets": "0.19.3",
    "@codemirror/rectangular-selection": "0.19.1",
    "@codemirror/search": "0.19.5",
    "@codemirror/state": "0.19.6",
    "@codemirror/theme-one-dark": "0.19.1",
    "@codemirror/view": "0.19.37",
    "@dnd-kit/core": "5.0.1",
    "@dnd-kit/sortable": "6.0.0",
    "@dnd-kit/utilities": "3.1.0",
    "@heroicons/react": "1.0.5",
    "@trpc-playground/query-extension": "workspace:0.0.0",
    "@trpc-playground/typescript-extension": "workspace:0.0.0",
    "@trpc/client": "9.16.0",
    "@trpc/react": "9.16.0",
    "clsx": "1.1.1",
    "jotai": "1.5.0",
    "object-inspect": "1.12.0",
    "react-input-autosize": "3.0.0",
    "react-query": "3.34.7",
    "rodemirror": "1.6.6",
    "sucrase-browser": "3.16.0"
  },
  "peerDependencies": {
    "preact": "^10"
  },
  "devDependencies": {
    "@trpc-playground/types": "workspace:*",
    "@trpc/server": "9.16.0",
    "@types/object-inspect": "1.8.1",
    "@types/react": "17.0.38",
    "@types/react-input-autosize": "2.2.1",
    "autoprefixer": "10.4.1",
    "postcss": "8.4.5",
    "postcss-prefix-selector": "1.14.0",
    "tailwindcss": "3.0.8"
  }
}
